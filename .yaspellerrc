{
  "excludeFiles": [
    ".*",
    "LICENSE",
    "README.md",
    "Gemfile",
    "_config.yml"
  ],
  "lang": "en",
  "fileExtensions": [
    ".md",
    ".css"
  ],
  "report": ["console", "html"],
  "dictionary": [
    "Jekyll",
    "Github",
    "Huxpro",
    "DIY"
  ],
  "ignoreText": [
    "header_img: /assets/img/*",
    "permalink: *",
    "hide_in_nav: *"
  ],
  "ignoreTags": ["code", "script"],
  "ignoreUrls": true,
  "ignoreCapitalization": true,
  "ignoreDigits": true,
  "maxRequests": 5
}
